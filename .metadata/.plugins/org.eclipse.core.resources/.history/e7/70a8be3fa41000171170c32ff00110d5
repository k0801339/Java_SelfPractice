class NoSuchIDExceptions extends Exception{
	public NoSuchIDExceptions(int ID){
		super("ERROR:"+ID);
	}
}

class NoSuchCommandExceptions extends Exception{
	public NoSuchCommandExceptions(String cmd){
		super("ERROR:"+cmd);
	}	
}