package cc.openhome;
class Queen{
	static int count = 0;
	int[] q = new int[8];
	public Queen(){
		
	}
	public void place(int row){
		if(row==8){
			display();
			count++;
		}
	}
	public int valid(int row, int col){
		
	}
	public void display(){
		for(int i=0;i<8;i++){
			for(int j=0;j<8;j++){
				if(q[i]==j)	System.out.print("@");
				else System.out.print("0");
			}
			System.out.println();
		}
		System.out.println();
	}
}

public class Ex4 {

	public static void main(String[] args) {
		

	}

}
