package pro.gradeSystem;

public class Grades {
	private int ID;
	String name;
	int lab1, lab2, lab3, midTerm, finalExam, totalGrade;
	public Grades(int ID,String name,int lab1,int lab2,int lab3,int midTerm,int finalExam){
		this.ID = ID;
		this.name = name;
		this.lab1 = lab1;
		this.lab2 = lab2;
		this.lab3 = lab3;
		this.midTerm = midTerm;
		this.finalExam = finalExam;
	}
}
